{
	"name": "ParquetTables",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Schema": {
				"type": "string",
				"defaultValue": "easy_ipl_db"
			},
			"tables": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@{concat(dataset().tables, '.parquet')}",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@{concat(dataset().Schema, '/', dataset().tables)}",
					"type": "Expression"
				},
				"fileSystem": "bronze"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}